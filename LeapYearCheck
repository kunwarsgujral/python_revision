year = int(input("Enter a year that you would like to find out if it it Leap Year or Not? "))
if(year % 4 == 0):
  if(year % 100 != 0):
    print(f"{year} is a Leap Year")
  elif(year % 400 != 0):
     print(f"{year} is not a Leap Year")
  else:
    print(f"{year} is a Leap Year")
else: 
  print(f"{year} is not Leap Year")
